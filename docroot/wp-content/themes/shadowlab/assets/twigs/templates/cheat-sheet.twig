{% extends "base.twig" %}

{% block content %}
  <header><h1 id="entry-title">{{ page.plural }}</h1></header>

  <cheat-sheet
    type="{{ page.singular }}"
    headers-json='{{ page.headers | json_encode }}'
    entries-json='{{ page.entries | json_encode }}'
    searchbar-json='{{ page.searchbar | json_encode }}'
  ></cheat-sheet>

{% endblock %}